// News

.news-box
    margin-left: 0
    min-height: 1px
    padding-right: 0px

    .list
        height: 85px
        margin: 0
        overflow: auto
        padding: 0
        -webkit-overflow-scrolling: touch

    .news-item
        margin-bottom: 4px

        .title p
            display: inline

        .date
            margin-right: 10px
            font-weight: bold
